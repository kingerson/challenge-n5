{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "MinimalApi-Local"
    },
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName",
      "WithMachineName"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=172.23.16.1,1433;Database=DbTest;Persist Security Info=False;User Id=sa;Password=Facil123.;TrustServerCertificate=True"
  },
  "JwtSecurityToken": {
    "Key": "super-secret-key-value!G3rs0nN@v@rr0",
    "Issuer": "SecurityAPI",
    "Audience": "MsCleanAPI",
    "Subject": "JWTProvider"
  },
  "KafkaConfig": {
    "BootstrapServers": "localhost:29092"
  },
  "ElasticSearchConfig": {
    "Url": "http://host.docker.internal:9200",
    "DefaultIndex": "permissions"
  }
}
